{
    "name": "analytodon-cli",
    "version": "0.0.0",
    "description": "Analytodon CLI Tools",
    "author": "Raphael StÃ¤bler @blazer82",
    "bin": {
        "analytodon-cli": "./bin/run"
    },
    "homepage": "https://github.com/blazer82/analytodon-cli",
    "license": "MIT",
    "main": "dist/index.js",
    "repository": "blazer82/analytodon-cli",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@newrelic/pino-enricher": "^1.1.1",
        "@oclif/core": "^1.19.1",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-plugins": "^2.1.7",
        "axios": "^0.27.2",
        "megalodon": "^4.1.3",
        "mongodb": "^4.12.1",
        "newrelic": "^10.6.2",
        "pino": "^8.15.0",
        "timezone-support": "^3.1.0"
    },
    "devDependencies": {
        "@oclif/test": "^2.2.12",
        "@types/chai": "^4",
        "@types/mocha": "^9.0.0",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "chai": "^4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.3",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^9",
        "oclif": "^3",
        "prettier": "^2.8.4",
        "shx": "^0.3.3",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3"
    },
    "oclif": {
        "bin": "analytodon-cli",
        "dirname": "analytodon-cli",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    },
    "scripts": {
        "build": "rm -rf dist && tsc -b && oclif manifest && oclif readme",
        "prettier:check": "npx prettier --check '{src,test}/**/*.ts'",
        "prettier:write": "npx prettier --write '{src,test}/**/*.ts'",
        "lint": "eslint . --ext .ts --config .eslintrc && npm run prettier:check",
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "npm run lint",
        "pack": "npm run build && oclif pack tarballs --no-xz -t linux-x64 && npm run postpack",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md"
    },
    "engines": {
        "node": ">=16.0.0 <19"
    },
    "bugs": "https://github.com/blazer82/analytodon-cli/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
